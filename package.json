    "build": "ng build",
    "start": "ng serve",
    "test": "ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "dev:ssr": "ng run asecan:serve-ssr",
    "prerender": "ng run asecan:prerender",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && gzip-all \"dist/browser/*.*\" && ng run asecan:server:production --bundleDependencies none",
    "dev": "npm run build:client-and-server-bundles && npm run compile:server && npm run serve:ssr"
  },
